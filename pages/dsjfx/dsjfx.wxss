/* pages/dsjfx/dsjfx.wxss */
.dsjfx{
    height: 230rpx;
    padding: 20rpx;
}
.dsjfx .left{
    float: left;
}
.dsjfx .left .top, .dsjfx .right .top{
    color: #5ddbff;
    font-size: 32rpx;
    font-weight: 400;
}
.dsjfx .left .con{
    margin-top: 20rpx;
}
.dsjfx .left .con .li{
    font-size: 28rpx;
    font-weight: 400;
    line-height: 60rpx;
    color: white;
}
.dsjfx .left .con .li .li-title{
    width: 120rpx;
    display: inline-block;
}
.dsjfx .left .con .li .li-content{
    color: #00ffba;
    width: 45rpx;
    display: inline-block;
}
.dsjfx .right{
    float: right;
    width: 430rpx;
}
.dsjfx .right .top{
    text-align: center;
}
.dsjfx .right .con{
    position: relative;
    width: 425rpx;
    height: 300rpx;
    margin-top: 20rpx;
}
.dsjfx .right .con .img{
    color: white;
    font-size: 32rpx;
    font-weight: 400;
    text-align: center;
    position: absolute;
}
.dsjfx .right .con view{
    position: absolute;
}
